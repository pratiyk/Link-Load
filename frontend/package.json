{
  "name": "linkload-frontend",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@supabase/supabase-js": "^2.81.1",
    "@tanstack/react-query": "^5.36.0",
    "@tremor/react": "^3.18.7",
    "axios": "^1.6.0",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.518.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.10",
    "react-circular-progressbar": "^2.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-hot-toast": "^2.6.0",
    "react-router-dom": "^6.23.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "test:watch": "react-scripts test --env=jsdom --watch",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "autoprefixer": "^10.4.19",
    "http-proxy-middleware": "^2.0.9",
    "jest": "^29.7.0",
    "msw": "^2.2.3",
    "postcss": "^8.4.38",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^9.3.0",
    "tailwindcss": "^3.4.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/App.js",
      "src/pages/Home.jsx",
      "src/pages/ScanResults.jsx",
      "src/components/SeverityBadge.js",
      "src/components/RemediationCard.js",
      "src/components/ErrorBoundary.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(axios|d3|d3-[^/]+|internmap|msw|@mswjs|@tanstack|until-async|delaunator|robust-predicates)/)"
    ]
  }
}
